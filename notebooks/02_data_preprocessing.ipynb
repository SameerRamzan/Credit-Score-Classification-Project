    "    print(\"Using MinMaxScaler (range 0-1)\")\n",
    "elif scaling_method == 'RobustScaler':\n",
    "    scaler = RobustScaler()\n",
    "    print(\"Using RobustScaler (median and IQR)\")\n",
    "else:\n",
    "    scaler = StandardScaler()\n",
